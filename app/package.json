{
  "name": "gulp-app",
  "productName": "Gulp",
  "version": "0.0.1",
  "description": "gulp as an app (OS X)",
  "license": "MIT",
  "repository": "fritx/gulp-app",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "contributors": [
    "Fabrice Weinberg <Fabrice@weinberg.me>"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . $npm_package_productName --version=0.31.0 --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager",
    "build_": "electron-packager . $npm_package_productName --prune --asar --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager"
  },
  "keywords": [
    "electron-app",
    "gulp",
    "gulpfile",
    "buil",
    "tool",
    "task",
    "runner",
    "app",
    "ui",
    "osx"
  ],
  "private": true,
  "dependencies": {
    "bufferhelper": "^0.2.0",
    "findup-sync": "^0.2.1",
    "fix-path": "^1.0.0",
    "gulp": "^3.9.0",
    "lodash": "^3.8.0",
    "resolve-from": "^1.0.0"
  },
  "devDependencies": {
    "electron-prebuilt": "0.31.0",
    "electron-packager": "^5.0.2"
  }
}
